  routes,
});

router.beforeEach((to, from, next) => {
  //准备跳到登录页放行
  // if (to.path === "/login") return next();
  // if (to.path === "/register") return next();
  let token = localStorage.getItem("token");
  //未登录，没有token
  if (token == "" || token == null) {
    if (to.path == "/login" || to.path == "/register") return next();
    console.log("请先登录");
    next("/login");
    // next({ name: "login" });
  } else {
    next();
  }
});

export default router;
